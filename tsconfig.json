{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      // libraries
      "@ui/*": [
        "./dist/ui/*"
      ],
      "@app/*": [
        "./src/app/*"
      ],
      "@store/*": [
        "app/store/*"
      ],
      "@core/*": [
        "app/core/*"
      ],
      "@shared/*": [
        "app/shared/*"
      ],
      "@layout/*": [
        "app/layout/*"
      ],
      "@models/*": [
        "app/core/models/*"
      ],
      "@pages/*": [
        "app/pages/*"
      ],
      "@auth/*": [
        "app/pages/auth/*"
      ],
      "@members/*": [
        "app/pages/members/*"
      ],
      "@settings/*": [
        "app/pages/settings/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
